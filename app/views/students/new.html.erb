<!-- Create your form here -->
<h1>Student Form</h1>
<%= form_tag students_path do %>
  <%= label_tag :'student[:first_name]' %>
  <br>
  <%= text_area_tag :first_name %>
  <br>
  <%= label_tag :'student[:last_name]' %>
  <br>
  <%= text_area_tag :last_name %>
  <br>
  <%= submit_tag 'Submit Student' %>
<% end %>